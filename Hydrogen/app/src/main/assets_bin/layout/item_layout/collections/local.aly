{
  LinearLayout;
  layout_width="fill";
  orientation="horizontal";
  BackgroundColor=backgroundc;
  {
    MaterialCardView;
    layout_gravity="center";
    layout_height="-2";
    CardBackgroundColor=cardedge,
    Elevation="0";
    layout_width="-1";
    layout_margin="16dp";
    layout_marginTop="8dp";
    layout_marginBottom="8dp";
    radius="8dp";
    StrokeColor=cardedge;
    StrokeWidth=dp2px(1),
    {
      LinearLayout;
      layout_height="fill";
      id="background";
      layout_width="fill";
      ripple="圆自适应",
      {
        LinearLayout;
        orientation="horizontal";
        padding="16dp";
        {
          TextView;
          id="cid";
          textSize="0sp";
        };
        {
          LinearLayout;
          orientation="vertical";
          {
            TextView;
            textSize="14sp";
            id="catitle";
            textColor=textc;
            layout_marginTop="2dp";
            Typeface=字体("product-Bold");
          };
        };
      };
    };
  };
};